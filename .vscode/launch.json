{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch(Cpp)",
      "type": "cppdbg",
      "request": "launch",
      "program": "${fileDirname}/a",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${fileDirname}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "gdb の再フォーマットを有効にする",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        },
        {
          "description": "逆アセンブリ フレーバーを Intel に設定",
          "text": "-gdb-set disassembly-flavor intel",
          "ignoreFailures": true
        }
      ],
      "preLaunchTask": "C/C++: g++ アクティブなファイルのビルド"
    },
    {
      "name": "Launch(Pypy)",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "args": [
        "-Xfrozen_modules=off",
        "-m"
      ],
      "env": {
        "PYTHONPATH": "/home/lit-to/atcoder:$PYTHONPATH"
      },
      "preLaunchTask": "export pypy_path"
    },
    {
      "name": "CppAuto",
      "type": "cppdbg",
      "request": "launch",
      "args": [],
      "stopAtEntry": false,
      "externalConsole": false,
      "cwd": "/home/lit-to/atcoder/contest/ABC381",
      "program": "/home/lit-to/atcoder/contest/ABC381/build/Debug/outDebug",
      "MIMode": "gdb",
      "miDebuggerPath": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}